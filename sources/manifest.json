{
    "manifest_version": 2,

    "name": "WalkMe checker",
    "version": "1.0",
    "icons": {
		"48": "48x48.png"
    },

	"permissions": [
		"tabs",
		"<all_urls>"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://s3.amazonaws.com; object-src 'self'",
  
    "browser_action": {
        "default_title": "Check WalkMe",
        "default_icon": "48x48.png",
        "default_popup": "popup.html"
    }
}
